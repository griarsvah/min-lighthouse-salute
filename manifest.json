{
  "manifest_version": 3,
  "name": "Minimal Manifest",
  "short_name": "MM",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
  "theme_color": "#FFFFFF",
  "background_color": "#FFFFFF",
  "display": "fullscreen",
  "id": "/?homescreen=1",
  "start_url": "/?homescreen=1",
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "https://griarsvah.github.io/PWABuilder/images/android/launchericon-144.png",
      "type": "image/png",
      "sizes": "144x144",
      "purpose": "any"
    },
    {
      "src": "https://griarsvah.github.io/PWABuilder/images/maskable/icon_x384.png",
      "type": "image/png",
      "sizes": "384x384",
      "purpose": "maskable"
    },
    {
      "src": "https://griarsvah.github.io/PWABuilder/images/ios/512.png",
      "type": "image/png",
      "sizes": "512x512",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "https://image-placeholder.com/images/actual-size/320x320.png",
      "type": "image/png",
      "sizes": "320x320",
      "form_factor": "narrow",
      "label": "Homescreen of Awesome App"
    },
    {
      "src": "https://griarsvah.github.io/PWABuilder/images/maskable/icon_x384.png",
      "type": "image/png",
      "sizes": "384x384",
      "form_factor": "wide",
      "label": "Homescreen of Awesome App"
    }
  ]
}
